datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
    id          Int      @id @default(autoincrement())
    email       String   @unique
    name        String
    password    String  
    dob         DateTime?  
    birthCity   String?
    postalCode  String?
    gender      String?
    country     String?
    address     String?
}
